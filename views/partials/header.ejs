<header class="bg-white border-gray-200 px-2 sm:px-4 py-2.5 dark:bg-gray-800">
  <div class="container flex flex-wrap items-center justify-between mx-auto max-w-screen-xl">
    <a href="/" class="flex items-center gap-2">
        <!-- <img src="https://flowbite.com/docs/images/logo.svg" class="h-6 mr-3 sm:h-9" alt="Flowbite Logo" /> -->
        <img src="/imgs/favicon-32x32.png" alt="Ticketing System logo">
        <span class="self-center text-xl font-semibold whitespace-nowrap dark:text-white">Ticketing System</span>
    </a>
    <div class="flex">
        <button id="theme-toggle" type="button" class="text-gray-500 dark:text-white hover:text-black dark:hover:text-yellow-500 rounded-lg text-sm p-2.5">
          <svg id="theme-toggle-dark-icon" class="hidden w-5 h-5" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M17.293 13.293A8 8 0 016.707 2.707a8.001 8.001 0 1010.586 10.586z"></path></svg>
          <svg id="theme-toggle-light-icon" class="hidden w-5 h-5" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M10 2a1 1 0 011 1v1a1 1 0 11-2 0V3a1 1 0 011-1zm4 8a4 4 0 11-8 0 4 4 0 018 0zm-.464 4.95l.707.707a1 1 0 001.414-1.414l-.707-.707a1 1 0 00-1.414 1.414zm2.12-10.607a1 1 0 010 1.414l-.706.707a1 1 0 11-1.414-1.414l.707-.707a1 1 0 011.414 0zM17 11a1 1 0 100-2h-1a1 1 0 100 2h1zm-7 4a1 1 0 011 1v1a1 1 0 11-2 0v-1a1 1 0 011-1zM5.05 6.464A1 1 0 106.465 5.05l-.708-.707a1 1 0 00-1.414 1.414l.707.707zm1.414 8.486l-.707.707a1 1 0 01-1.414-1.414l.707-.707a1 1 0 011.414 1.414zM4 11a1 1 0 100-2H3a1 1 0 000 2h1z" fill-rule="evenodd" clip-rule="evenodd"></path></svg>
        </button>
        <%if(user !== ''){ %>
          <!-- <span class="text-gray-700 dark:text-gray-400 font-medium text-sm px-5 py-2.5 text-center mr-3 md:mr-0 cursor-default order-1">Hello, <%=user.userName %></span> -->
          <a href="/logout" type="button" class="text-white bg-indigo-700 hover:bg-indigo-800 focus:ring-4 focus:outline-none focus:ring-indigo-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center mr-3 md:mr-0 dark:bg-indigo-600 dark:hover:bg-indigo-700 dark:focus:ring-indigo-800">Sign out</a>
        <% } %>
    </div>
  </div>
</header>



    


